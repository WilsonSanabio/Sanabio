<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sanabios.bhaskara" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Bhaskara</name>
    <description>
        Cálculo da Equação de 2º Grau
    </description>
    <author email="wilsonsanabio@hotmail.com" href="http://phonegap.com">
        Sana/Bios Informática &amp; Consultoria
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
